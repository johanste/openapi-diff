{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/src/index.js",
      "cwd": "${workspaceFolder}",
      "args": [
        "--lhs",
        "KeyVaultOriginal/common.json",
        "KeyVaultOriginal/secrets.json",
        "--rhs",
        "KeyVaultGenerated",
        "--compile-tsp",
        "--group-violations"
      ]
    }
  ]
}
